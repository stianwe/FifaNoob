CREATE TABLE mmatch (
    id INT AUTO_INCREMENT NOT NULL,
    homePId INT NOT NULL,
    awayPId INT NOT NULL,
    homeGoals INT NOT NULL,
    awayGoals INT NOT NULL,
    time BIGINT,
    PRIMARY KEY (id)
) ENGINE=INNODB;

CREATE TABLE player (
    id INT AUTO_INCREMENT NOT NULL,
    name VARCHAR(30) NOT NULL,
    PRIMARY KEY (id)
) ENGINE=INNODB;
